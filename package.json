{
	"name": "gitlios",
	"displayName": "GitLios",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gitlios.helloWorld",
				"title": "GitLios: Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"compile-webpack": "npm run prod",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"dev": "webpack --mode development",
		"prod": "webpack --mode production",
		"watch-webpack": "webpack --mode development --watch"
	},
	"dependencies": {
		"git-stat-common": "file:../gitlios"
	},
	"devDependencies": {
		"@types/glob": "7.1.1",
		"@types/mocha": "7.0.2",
		"@types/node": "12.12.31",
		"@types/vscode": "1.43.0",
		"eslint": "6.8.0",
		"@typescript-eslint/parser": "2.25.0",
		"@typescript-eslint/eslint-plugin": "2.25.0",
		"glob": "7.1.6",
		"mocha": "7.1.1",
		"typescript": "3.7.5",
		"vscode-test": "1.3.0",
		"webpack": "4.42.1",
		"webpack-cli": "3.3.11",
		"ts-loader": "6.2.2"
	}
}
